




























partial default alphanumeric_keys
xkb_symbols "sun_type6" {

 name[Group1]= "Russian (Sun Type 6/7)";

 include "sun_vndr/us(sun_type6)"

 key.type[group1]="FOUR_LEVEL";

 key <AE01> { [ 1, exclam, 1, exclam ] };
 key <AE02> { [ 2, quotedbl, 2, at ] };
