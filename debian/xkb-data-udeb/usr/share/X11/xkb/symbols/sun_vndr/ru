 key <BKSL> { [ slash, question, backslash, bar ] };
 key <AB10> { [ ediaeresis, Ediaeresis, slash, question ] };
 key <LSGT> { [ backslash, bar, backslash, bar ] };

 key <AD01> { [ Cyrillic_shorti, Cyrillic_SHORTI, q, Q ] };
