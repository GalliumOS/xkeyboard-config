xkb_keycodes "type6tuv" {
 include "sun(type6)"
 <LSGT> = 131;
};

xkb_keycodes "type6_jp" {
 include "sun(type6)"
 <EXEC> = 122;
 <KANJ> = 123;
 <HENK> = 124;
 <UNDR> = 118;
};

