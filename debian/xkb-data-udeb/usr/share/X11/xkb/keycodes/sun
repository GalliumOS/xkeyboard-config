};




default xkb_keycodes "type6_usb" {
 include "xfree86"












 

 
 


 







 <I150> = 155;
 <I160> = 160;
 <I235> = 235;

 indicator 4 = "Compose";
};

xkb_keycodes "type6tuv_usb" {
 include "sun(type6_usb)"


};

xkb_keycodes "type6_jp_usb" {
 include "sun(type6_usb)"
 alias <UNDR> = <KPDC>; 
 alias <EXEC> = <NFER>; 
 alias <KANJ> = <XFER>; 
 alias <HENK> = <HKTG>; 
 indicator 5 = "Kana";
};
